# LiveKit Configuration
LIVEKIT_URL=wss://your-livekit-url.livekit.cloud
LIVEKIT_API_KEY=your-api-key
LIVEKIT_API_SECRET=your-api-secret
LIVEKIT_ROOM=demo
LIVEKIT_IDENTITY=pipeline-agent

# LiveKit Audio Settings (Optional)
LIVEKIT_AUDIO_SAMPLE_RATE=16000
LIVEKIT_AUDIO_CHANNELS=1
LIVEKIT_AUDIO_FRAME_MS=20

# Gemini Configuration
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-2.0-flash-exp

# ElevenLabs Configuration
ELEVENLABS_API_KEY=your-elevenlabs-api-key
ELEVENLABS_VOICE_ID=your-voice-id
ELEVENLABS_STT_MODEL_ID=scribe_v2
ELEVENLABS_TTS_MODEL_ID=eleven_turbo_v2_5
ELEVENLABS_STT_LANGUAGE=en

# Deepgram Configuration (Optional backup STT provider)
DEEPGRAM_API_KEY=your-deepgram-api-key

# STT Settings (Optional)
STT_SEGMENT_SECONDS=2
TRANSCRIPTION_FLUSH_MS=300

# TTS Publishing Settings (Optional)
PUBLISH_TTS_TO_ROOM=false
PUBLISH_TTS_AUDIO_TRACK=true
TTS_AUDIO_SAMPLE_RATE=16000
TTS_AUDIO_CHANNELS=1
TTS_AUDIO_FRAME_MS=20
TTS_AUDIO_TRACK_NAME=aura-tts

# Wake/Sleep Phrases
WAKE_PHRASE=hey aura
SLEEP_PHRASE=bye

# Bridge WebSocket Server (for codingterminal integration)
# Port for the WebSocket server that connects to codingterminal
BRIDGE_WS_PORT=8765
